#!/bin/bash
if test $# -ne 1
then
	echo Usage: $0 contest_path
	echo "      eg. $0 /home/ubuntu/repos/wfluxor/contests/wf46/systest1"
	exit 1
fi
cpath="$1"
if test ! -d ${cpath}
then
	echo Invalid contest path: ${cpath}
	exit 2
fi
cd ~/pc2
rm current
ln -s ${cpath} current
ls -l ~/pc2/current
fej 'cd pc2; rm current; rm -rf logs ?log; ln -s "'${cpath}'" current; ls -l ~/pc2/current'
